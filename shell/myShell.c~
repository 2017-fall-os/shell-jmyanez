#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

int stringComp(char* cmd1, char* cmd2){
 
  printf("cmd1: %c\n", cmd1[5]);
  
  printf("cmd2: %c\n", cmd2[5]);
  for(int i=0; cmd1[i]!='\0';i++){
    
  if(cmd1[i] != cmd2[i]){
      return 0;
  }
  }
   return 1 ;

}

int main(){
 
   char cmd1[30];
   int flag =0;
   
  while(flag!=1){
    write(1,"$",1);
     read(0, cmd1,30);
    flag = stringComp(cmd1,"exit");
	
  }
  return 1;
}
